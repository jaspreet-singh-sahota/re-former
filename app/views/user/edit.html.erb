<%= form_for @user, url: { action: 'update'} do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username , value: @user.username %> <br>
  <%= f.label :email %>
  <%= f.text_field :email , value: @user.email %> <br>
  <%= f.label :password %>
  <%= f.password_field :password,value: @user.password %> <br>
  <%= f.submit "submit" %>
<%end%>